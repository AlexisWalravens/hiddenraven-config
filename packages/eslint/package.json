{
  "name": "@hiddenraven/eslint-config",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexisWalravens/hiddenraven-config/tree/main/packages/eslint"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "./base": "./src/base.js",
    "./react": "./src/react.js",
    "./nextjs": "./src/nextjs.js",
    "./turbo": "./src/turbo.js"
  },
  "main": "./src/base.js",
