    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "rc",
    "eslint-plugin-turbo": "^2.1.1",
    "typescript-eslint": "^8.3.0"
  },
  "devDependencies": {
    "@hiddenraven/prettier-config": "workspace:*",
    "@hiddenraven/tsconfig": "workspace:*",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4"
